{
  "version": 2,
  "functions": {
    "api/track.js": {
      "maxDuration": 10
    },
    "api/analytics.js": {
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/track",
      "dest": "/api/track.js"
    },
    {
      "src": "/api/analytics",
      "dest": "/api/analytics.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/tracking-config.json",
      "destination": "/tracking-config.json"
    }
  ]
}